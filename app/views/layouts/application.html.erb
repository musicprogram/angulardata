

<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>comunidadSena</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %> 
	
	<%= csrf_meta_tags %>
	<link rel="stylesheet" href="<link href='http://fonts.googleapis.com/css?family=Ubuntu:400,500,700' rel='stylesheet' type='text/css'>">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.2/normalize.min.css">
	<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
</head>
<body class="<%= @home_page ? 'homepage' : ''%>">

	<% if user_signed_in? %>
	<div class="main_header clear_fix">

		<div class="wrapper1">


			<nav class="navbar navbar-default" role="navigation">
				<!-- Brand and toggle get grouped for better mobile display -->
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button >
					<div>
						
					<%= link_to "<div class='fa fa-diamond fa-4x'></div>".html_safe, root_path, class: 'navbar-brand '%>
					</div>

				</div>
				
				
				
				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse navbar-ex1-collapse">
					<ul class="nav navbar-nav navbar-right">
						
					
						<li class="dropdown">
				        <a href="#" class="dropdown-toggle  fa fa-pencil-square fa-2x" data-toggle="dropdown">
				           <b class="caret"></b>
				        </a>
				        <ul class="dropdown-menu">
				          	
											
						<li><%= link_to "Post", posts_path, class: 'active'%></li>
						<li><%= link_to "New Post", new_post_path %></li>
				          
				        </ul>
				      </li>
				      
				      <li class="dropdown">
				        <a href="#" class="dropdown-toggle  fa fa-camera fa-2x" data-toggle="dropdown">
				           <b class="caret"></b>
				        </a>
				        <ul class="dropdown-menu">
				          	
										
						<li><%= link_to "Pin", pins_path, class: 'active'%></li>
						<li><%= link_to "New Pin", new_pin_path %></li>
				          
				        </ul>
				      </li>
				      
				      <li class="dropdown">
				        <a href="#" class="dropdown-toggle  fa fa-user fa-2x" data-toggle="dropdown">
				           <b class="caret"></b>
				        </a>
				        <ul class="dropdown-menu">
				          	
										
						<li><%= link_to "Sign Out", destroy_user_session_path, :method => :delete, class: 'active' %></li>
				         
				          
				        </ul>
				      </li>
					
						
						
					
					</ul>
					
				</div><!-- /.navbar-collapse -->
				
				
				
				        
						
					
				          
			</nav>

			
		</div>	


	</div>
	<% end %> 
 


	<div class="container invisible">
		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>
	</div>

	

	<div class="container">
		<%= yield%>
	</div>
</body>


</html>



      


<script>
	$('.dropdown-toggle').dropdown()
</script>
